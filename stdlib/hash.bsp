# 64-bit murmur OOAT hash
[FUNC @HASH [[PARAM !m $STRING] [RETURNS [INT]]] [
  [DEFINE !h 525201411107845655]
  [DEFINE !i 0]
  [WHILE [< !i [LEN !m]] [
    [DEFINE !h [XOR !h [TOI [GET !m !i]]]]
    [DEFINE !h [* !h 6616326155283851669]]
    [DEFINE !h [SHIFT !h -47]]
    [DEFINE !i [+ !i 1]]
  ]]
  [RETURN !h]
]]