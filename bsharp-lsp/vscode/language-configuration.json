{
	"comments": {
		"lineComment": "#"
	},
	"brackets": [
		["[", "]"]
	],
	"autoClosingPairs": [
		{
			"open": "{",
			"close": "}",
			"notIn": ["string", "comment"]
		},
		{
			"open": "[",
			"close": "]",
			"notIn": ["string", "comment"]
		},
		{
			"open": "\"",
			"close": "\"",
			"notIn": ["string"]
		},
		{
			"open": "'",
			"close": "'",
			"notIn": ["string"],
		}
	],
	"surroundingPairs": [
		["[", "]"],
		["\"", "\""]
	],
	"folding": {
		"markers": {
			"start": "^(?:\\[IF|\\[WHILE|\\[SWITCH|\\[CASE|\\[FUNC).+",
			"end": "^]"
		}
	}
}