# Functions
[IMPORT "import.bsp"]
[IMPORT "math.bsp"]

[FUNC HELLO 
  [PRINT "Hello, World!"]
]

[HELLO]
[PRINT [STRING [ADD 1 2]]]

# Some basic Functions
[DEFINE a 1]
[PRINT [STRING [VAR a]]]
[PRINT [CONCAT "hello w" "orld"]]
[IF [COMPARE 6 != 4]
  [PRINT "6 is not 4"]
ELSE
  [PRINT "6 is 4, uh oh!"]
]

[PRINT [STRING [RANDINT 0 100]]]

[DEFINE abcdefg [FN HELLO]]

# Arrays
[DEFINE c [ARRAY 1 2 3]]
[PRINT [STRING [INDEX [VAR c] 0]]]
[APPEND [VAR c] 4]